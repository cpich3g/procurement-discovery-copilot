# Enviro# Model Configuration
LLM_MODEL=gpt-4  # Azure deployment name for your GPT-4 model
REASONING_MODEL=o3-mini  # Azure deployment name for your reasoning model (o3/o3-mini)
LLM_TEMPERATURE=0.1
LLM_MAX_TOKENS=2000
REASONING_MAX_TOKENS=4000
# Note: Reasoning models (o3, o3-mini, etc.) do not support custom temperature - they use default=1

# Use reasoning model for specific tasks
USE_REASONING_MODEL_FOR_ANALYSIS=true
USE_REASONING_MODEL_FOR_COMPLEX_SEARCH=truebles for Procurement Discovery Tool
# Copy this file to .env and fill in your API keys

# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_API_VERSION=2025-04-01-preview

# Model Configuration
LLM_MODEL=gpt-4.1  # Azure deployment name for your GPT-4 model
REASONING_MODEL=o3  # Azure deployment name for your reasoning model (o3/o3-mini)
LLM_TEMPERATURE=0.1
LLM_MAX_TOKENS=20000
REASONING_TEMPERATURE=0.0
REASONING_MAX_TOKENS=40000

# Use reasoning model for specific tasks
USE_REASONING_MODEL_FOR_ANALYSIS=true
USE_REASONING_MODEL_FOR_COMPLEX_SEARCH=true

# Tavily Search API Configuration  
TAVILY_API_KEY=
SEARCH_MAX_RESULTS=5
MAX_SEARCH_QUERIES=5

# LangSmith Tracing (Optional)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=procurement-discovery

# Application Configuration
DEBUG=false
LOG_LEVEL=INFO
WORKFLOW_MAX_RETRIES=3
WORKFLOW_TIMEOUT=300

# Performance Settings
MAX_CONCURRENT_REQUESTS=10
ENABLE_PARALLEL_PROCESSING=true
ENABLE_STATE_PERSISTENCE=true
CHECKPOINT_INTERVAL=1
